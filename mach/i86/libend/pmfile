local d = ROOTDIR.."mach/i86/libend/"

libend_i86 = acklibrary {
	outputs = {"%U%/libend-%PLATFORM%.a"},

	ackfile (d.."edata.s"),
	ackfile (d.."em_end.s"),
	ackfile (d.."end.s"),
	ackfile (d.."etext.s"),

	install = pm.install("%BINDIR%lib/%PLATFORM%/libend.a"),
}

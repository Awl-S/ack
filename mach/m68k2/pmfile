local d = ROOTDIR.."mach/m68k2/"

mach_m68k2 = group {
	ARCH = "m68k2",
	
	proto_as,
	proto_ncg { ARCHDIR = "m68020" },
	ego_descr,
	
	install = {
		pm.install("%ROOTDIR%/lib/%ARCH%/descr", "%BINDIR%%PLATIND%/%ARCH%/descr"),
	}
}



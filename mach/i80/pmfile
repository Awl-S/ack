-- $Source$
-- $State$

local d = ROOTDIR.."mach/i80/"

include (d.."libem/pmfile")
include (d.."libend/pmfile")

mach_i80 = group {
	ARCH = "i80",
	
	proto_as,
	proto_ncg { ARCHDIR = "i80" },
	
	install = pm.install("%ROOTDIR%/lib/%ARCH%/descr", "%BINDIR%%PLATIND%/%ARCH%/descr")
}

support_i80 = group {
	OPTIMISATION = "-O",

	libem_i80,
	libend_i80,
}


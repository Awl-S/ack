local d = ROOTDIR.."mach/i386/"

include (d.."cv/pmfile")

mach_i386 = group {
	ARCH = "i386",
	
	proto_as,
	proto_ncg { ARCHDIR = "i386" },
	ego_descr,
	tool_i386_cv,	
	
	install = pm.install("%ROOTDIR%lib/%ARCH%/descr", "%BINDIR%%PLATIND%/%ARCH%/descr"),
}


MACH=`(cd .. ; basename \`pwd\`)`
if cp $1 ../../../lib/${MACH}/$1 >/dev/null 2>&1 ||
	 { rm -f ../../../lib/${MACH}/$1 >/dev/null 2>&1 &&
           cp $1 ../../../lib/${MACH}/$1 >/dev/null 2>&1
	 }
then
	set -
	ranlib ../../../lib/${MACH}/$1 >/dev/null 2>&1
	exit 0
else
	echo Sorry, can not create "lib/${MACH}/$1".
fi

var w=2
var p=2
var s=2
var l=4
var f=4
var d=8
var NAME=i8080
var M=8080
var LIB=mach/8080/lib/tail_
var RT=mach/8080/lib/head_
var INCLUDES=-I{EM}/include -I/usr/include
name be
	from .m
	to .s
	program {EM}/lib/{M}_be
	args <
	prop >
	need .e
end
name asld
	from .s.a
	to a.out
	program {EM}/lib/{M}_as
	mapflag -l* LNAME={EM}/{LIB}*
	args ({RTS}:.c={EM}/{RT}cc) -o > <
	prop C
end

: ${CDIR=.}
${DF-:} $CDIR .distr
if test ! -r $DESTDIR/$CDIR/.distr
then
	echo ++ no .distr in $CDIR
        exit 0
fi
for i in `cat $DESTDIR/$CDIR/.distr`
do
        if test -d $i
        then
                ( if cd $i
		  then 
			${DD-:} $CDIR $i
			CDIR=$CDIR/$i
			export CDIR
			exec /proj/em/distr/dwalk
		  else
			echo ++ Could not access $CDIR/$i
		  fi
		)
	else
		${DF-:} $CDIR $i
        fi
done

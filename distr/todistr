REV=
FILE=
while :
do
	case $# in
	0)	break ;;
	esac
	ARG="$1"
	shift
	case "$ARG" in
	-r*)	REV=`echo "$ARG"| sed s/-r//` ;;
	-*)	FLAGS="$FLAGS $ARG" ;;
	*)	case x$FILE in
		x)	FILE="$ARG" ;;
		*)	echo todistr can only be done on one file at the time
			exit 1 ;;
		esac
	esac
done
case x$REV in
x)	REV=`rlog -h "$FILE"|sed -n -e '/head/s/^head:[ 	]*//p'` ;;
esac
case x$REV in
x)	exit 2 ;;
esac
rcs -ndistr3:$REV $FLAGS $FILE

#include <dec.ocm>
#include <prints.ocm>

var fmt[byte 100]:
var d, c:
seq
	input ? c
	decin(input, d, c)
	while c<>EOF
		seq
			chan link:
			par
				printd(link, "XXXX %%%ds XXXXX*#00", d)
				var c, i:
				seq
					i:=0
					link ? c
					while c<>0
						seq
							i:=i+1
							fmt[byte i]:=c
							link ? c
			prints("XXXX %s XXXXX", "YYYYY")
			decin(input, d, c)

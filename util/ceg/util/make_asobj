EM	= ../../..
CEG	= $(EM)/lib/ceg
CE_MAKE	= $(CEG)/util/make_ce
BACK_MAKE = $(CEG)/util/make_back
CEG_MAKE = $(CEG)/util/make_ceg_$(OBJ)


all:	back.a ceg/ceg ce.a

install:	all
	../../install ce.a
	../../install back.a

cmp:	all
	-../../compare ce.a
	-../../compare back.a

clean:
	rm -rf ce back ceg ce.a back.a Out

dclean:
	rm -rf ce back ceg Out

ce.a:
	make -f $(CE_MAKE)

back.a:
	make -f $(BACK_MAKE) "OBJ="$(OBJ)

ceg/ceg:
	make -f $(CEG_MAKE)

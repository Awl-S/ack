EMHOME=../../..
CEG=../../../lib/ceg/util

case $# in
  1) case $1 in
	-as)	MAKE=$CEG/make_asobj;OBJ=as;;
	-obj)	MAKE=$CEG/make_asobj;OBJ=obj;;
	   *) echo wrong option $1, use -as or -obj;
     	      exit 1;;
     esac;;

  0) MAKE=$CEG/make_own;OBJ=obj ;;


  *) echo wrong number of arguments;
     exit 1;;
esac
make -f $MAKE OBJ=$OBJ install

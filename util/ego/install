case $# in
1)	DEST="$1" ;;
2)	DEST="$2" ;;
*)	echo $0 [source] destination ;;
esac
mkdir ../../../lib/ego > /dev/null 2>&1
if cp "$1" ../../../lib/ego/$DEST >/dev/null 2>&1 ||
	 { rm -f ../../../lib/ego/$DEST >/dev/null 2>&1 &&
           cp "$1" ../../../lib/ego/$DEST >/dev/null 2>&1
	 }
then
	exit 0
else
	echo Sorry, can not create "lib/ego/$DEST".
	exit 1
fi
